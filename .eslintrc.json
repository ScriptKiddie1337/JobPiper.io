{
    "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true
    },
    "rules": {
    "eslint.packageManager": "yarn",
    "no-duplicate-case": "error",
    "no-empty": "error",
    "no-extra-semi": "error",
    "no-func-assign": "error",
    "no-irregular-whitespace": "error",
    "no-unreachable": "error",
    "curly": "error",
    "dot-notation": "error",
    "eqeqeq": "error",
    "no-empty-function": "error",
    "no-multi-spaces": "error",
    "no-unused-vars": "error",
    "camelcase": "error",
    "indent": [
    "error",
    "2"
    ],
    "linebreak-style": 0,
    "quotes": [
    "error",
    "single"
    ],
    "semi": [
    "error",
    "always"
    ]
    }
}